function Availability(name){
  this.indicator = 'off',
  this.name = name
}

Availability.prototype.indicator = function(indicator){
  console.log(`Status:  ${indicator}`)
}
  
function wattsAmount(name, watt){
  this.name = name,
  this.watt = watt
}

wattsAmount.prototype = new Availability()

wattsAmount.prototype.indicator = function(indicator){
  console.log(`Status:  ${indicator}`)
}

const computer = new wattsAmount('computer', 65)
const lamp = new wattsAmount('lamp', 5)

computer.indicator('on')
console.log(computer);
lamp.indicator('off')
console.log(lamp);
